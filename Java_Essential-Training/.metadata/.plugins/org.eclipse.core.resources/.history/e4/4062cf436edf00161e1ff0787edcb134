
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
           boolean b1= true;
           boolean b2= false;
           System.out.println("the value of b1 is :" + b1);
           System.out.println("the value of b2 is :" + b2);
           boolean b3=!b1;
           System.out.println("the value of b3 is :" + b3);
           
	}

}
