package com.lynda.olivepress;

public class Main {
	public static void main(String[] args) {
		ArrayList<Olive> olives = new ArrayList<Olive>();
		Olive olive= new Olive();
		System.out.println(olive.name);
		olives.add(olive);
		olive=new Olive();
		System.out.println(olive.name);
		olives.add(olive);
		olive= new Olive();
		System.out.println(olive.name);
		olives.add(olive);
		OlivePress press = new OlivePress();
		press.getOil(olives);
		
		
	}
}
